<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Company.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="465.0" prefWidth="617.0">
         <children>
            <Pane layoutX="10.0" layoutY="9.0" prefHeight="453.0" prefWidth="601.0">
               <children>
                  <Label layoutX="74.0" layoutY="118.0" prefHeight="17.0" prefWidth="68.0" text="Username">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font></Label>
                  <Label layoutX="76.0" layoutY="198.0" prefHeight="20.0" prefWidth="68.0" text="Password">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font></Label>
                  <TextField fx:id="username" layoutX="196.0" layoutY="123.0" prefHeight="25.0" prefWidth="196.0" />
                  <TextField fx:id="password" layoutX="196.0" layoutY="194.0" prefHeight="25.0" prefWidth="196.0" />
                  <Button layoutX="255.0" layoutY="260.0" mnemonicParsing="false" onAction="#loginClick" text="Login">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font></Button>
               </children>
            </Pane>
         </children>
      </AnchorPane>
   </children>
</GridPane>
